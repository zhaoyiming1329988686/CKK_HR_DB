
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>TuPianFJ</title>

    <link href="~/Scripts/css/table.css" rel="stylesheet" />
    <script src="~/Scripts/javascript/comm/comm.js"></script>
</head>
<body>
    <form method="post" action="~/human_register/TuPianFJCZ" enctype="multipart/form-data">
        <table width="100%">
            <tr>
                <td colspan="2">
                    <font color="black">您正在做的业务是：人力资源--人力资源档案管理--附件上传</font>
                </td>
            </tr>
            <tr>
                <td width="65%">
                    提交成功，如需上传附件，请选择上传(word文档、txt文档、pdf文档、jpg图片等，最大5.0MB)
                    <input type="file" class="FILE_STYLE1" name="file"
                           width="100%">
                        
                    
                </td>
                <td width="41%" align="right">
                    <input type="hidden" name="humanFilehumanId" value="@ViewBag.tupian" />
                    <input type="button" value="上传图片" class="BUTTON_STYLE1" onclick="location.href='~/human_register/TuPian/ttt=@ViewBag.tupian'" />
                    <input type="submit" value="完成" class="BUTTON_STYLE1" />
                </td>
            </tr>
        </table>
    </form>
</body>
</html>
